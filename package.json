  "main": "webpack.config.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=production env_config=dev webpack-dev-server --env=dev --progress --inline",
    "build:prod": "cross-env NODE_ENV=production env_config=prod webpack --env=prod -p --progress ",
    "build:test": "cross-env NODE_ENV=testproduction env_config=test  webpack --env=prod -p --progress ",
    "server": "node server.js",
    "format": "onchange 'src/**/*.js' -- prettier --write {{changed}}",
    "start":"npm run dev && npm run format"
  },
